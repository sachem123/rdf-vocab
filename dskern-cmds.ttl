#signature for Dataset ExpandTripleFunction
dskern:ExpandTripleFunctionSignature a dskern:Signature ;
	dskern:signatureArgs ( 
		[dskern:argType dskern:Entity; rdfs:label "instance-uri"; rdfs:comment "entity-uri to expand"@en ] 
		[dskern:argType xsd:string; rdfs:label "optionX=Y"; rdfs:comment "expression"@en ] 
	) ;
	dskern:signatureOption dskern:SignatureOption.RETURNTYPE ;
	dskern:signatureOption dskern:SignatureOption.RETURNVALUE ;
