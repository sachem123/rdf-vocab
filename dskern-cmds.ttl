# RDF Related
@prefix rdf:     <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .

# P&O Using Passport & foaf 
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .

# Names spaces for the Corpus
@prefix dspol:    <http://www.3ds.com/RDF/Corpus/Policy/> .
@prefix dskern:    <http://www.3ds.com/RDF/Corpus/Kernel/> .

#some default options
dskern:SignatureOption.URI a rdfs:Class , dskern:SignatureOptionWithValue ;
    dskern:argType rdfs:Resource ;
	dskern:commandKeywordValue "input-uri" ;
	rdfs:label "URI" ;
	rdfs:comment "URI to be passed as option"@en ;
    .
dskern:SignatureOption.PREFIX a rdfs:Class , dskern:SignatureOptionWithValue ;
    dskern:argType xsd:string ;
	dskern:commandKeywordValue "prefix-string" ;
	rdfs:label "PREFIX" ;
	rdfs:comment "PREFIX to be passed as option"@en ;
    .
dskern:SignatureOption.THIS a rdfs:Class , dskern:SignatureOptionWithValue ;
    dskern:argType rdfs:Resource ;
	dskern:commandKeywordValue "instance-uri" ;
	rdfs:label "THIS" ;
	rdfs:comment "The subject itself to be passed as option"@en ;
    .
dskern:SignatureOption.BLANK a rdfs:Class , dskern:SignatureOption ;
	rdfs:label "BLANK" ;
	rdfs:comment "Option to indicate if the element is a blank node"@en ;
    .
dskern:SignatureOption.TYPE a rdfs:Class , dskern:SignatureOptionWithValue ;
    dskern:argType rdfs:Class ;
	dskern:commandKeywordValue "t1,t2,..." ;
	rdfs:label "TYPE" ;
	rdfs:comment "Option to indicate URI of types"@en ;
    .
dskern:SignatureOption.SELECT a rdfs:Class , dskern:SignatureOptionWithValue ;
	dskern:argType rdf:Property ;
	dskern:commandKeywordValue "s1,s2,..." ;
	rdfs:label "SELECT" ;
	rdfs:comment "List of selectables"@en ;
    .
dskern:SignatureOption.FILTER a rdfs:Class , dskern:SignatureOptionWithValue ;
	dskern:argType rdfs:Resource ;
	dskern:commandKeywordValue "filter-uri" ;
	rdfs:label "FILTER" ;
	rdfs:comment "Option to provide URI to filter function"@en ;
    .
dskern:SignatureOption.DIRECTION a rdfs:Class , dskern:SignatureOptionWithValue ;
	dskern:argType [ owl:oneOf ( "to" "from" ) ] ;
	dskern:commandKeywordValue "to/from" ;
	rdfs:label "DIRECTION" ;
	rdfs:comment "Option to indicate direction to expand"@en ;
    .
dskern:SignatureOption.RECURSE a rdfs:Class , dskern:SignatureOptionWithValue ;
	dskern:argType xsd:int ;
	dskern:commandKeywordValue "recurse-level" ;
	rdfs:label "RECURSE" ;
	rdfs:comment "Option to indicate number of levels to expand"@en ;
    .
dskern:SignatureOption.BOUNDED a dskern:SignatureOption ;
	rdfs:label "BOUNDED" ;
	rdfs:comment "Option to indicate if operation should be concise bounded "@en ;
    .
dskern:SignatureOption.INVERSE a dskern:SignatureOption ;
	rdfs:label "INVERSE" ;
	rdfs:comment "Option to indicate is the direction to traverse is reverse"@en ;
    .
dskern:SignatureOption.SYMMETRIC a dskern:SignatureOption ;
	rdfs:label "SYMMETRIC" ;
	rdfs:comment "Option to indicate if operation should be symmetric concise bounded"@en ;
    .
dskern:SignatureOption.PREVENTDUPLICATES a dskern:SignatureOption ;
	rdfs:label "PREVENTDUPLICATES" ;
	rdfs:comment "Option to indicate if duplicate results are disallowed in expand"@en ;
    .
dskern:SignatureOption.TARGETTYPE a rdfs:Class , dskern:SignatureOptionWithValue ;
	dskern:argType rdfs:Class ;
	dskern:commandKeywordValue "type1,type2,..." ;
	rdfs:label "TARGETTYPE" ;
	rdfs:comment "Option to indicate list of URI of target types"@en ;
    .
dskern:SignatureOption.TARGETSELECT a rdfs:Class , dskern:SignatureOptionWithValue ;
	dskern:argType rdf:Property ;
	dskern:commandKeywordValue "property1,property1,..." ;
	rdfs:label "TARGETSELECT" ;
	rdfs:comment "Option to indicate list of URI of tatrget properties"@en ;
    .
dskern:SignatureOption.PROPERTY a rdfs:Class , dskern:SignatureOptionWithValue ;
	dskern:argType rdf:Property ;
	dskern:commandKeywordValue "property1,property1,..." ;
	rdfs:label "PROPERTY" ;
	rdfs:comment "Option to indicate list of URI of properties"@en ;
    .
dskern:SignatureOption.FORMAT a rdfs:Class , dskern:SignatureOptionWithValue ;
    dskern:argType [ owl:oneOf ( "prefix" "len" "postfix" ) ] ;
	dskern:commandKeywordValue "prefix/len/postfix" ;
	rdfs:label "FORMAT" ;
	rdfs:comment "Option to indicate autonamer format"@en ;
    .
dskern:SignatureOption.RETURNTYPE a rdfs:Class , dskern:SignatureOptionWithValue ;
    dskern:argType [ owl:oneOf ( "single" "list" "iterator" "map" "mapofsingle" ) ] ;
	dskern:commandKeywordValue "single/list/iterator/map/mapofsingle" ;
	rdfs:label "RETURNTYPE" ;
	rdfs:comment "Type that the data should be returned as"@en ;
    .	
dskern:SignatureOption.RETURNVALUE a dskern:SignatureOption ;
	rdfs:label "RETURNVALUE" ;
	rdfs:comment "indicates that the result should be returned as non-node literal values"@en ;
    .	
dskern:SignatureOption.TFORMAT a rdfs:Class , dskern:SignatureOptionWithValue ;
    dskern:argType [ owl:oneOf ( "ams" "hdt" "ttl" ) ] ;
	dskern:commandKeywordValue "ams/hdt/ttl" ;
	rdfs:label "FORMAT" ;
	rdfs:comment "Triple format to use"@en ;
    .	
dskern:SignatureOption.FILE a rdfs:Class , dskern:SignatureOptionWithValue ;
    dskern:argType xsd:string ;
	rdfs:label "FILE" ;
	rdfs:comment "Output file where to write triples"@en ;
    .	
dskern:SignatureOption.TOGRAPH a rdfs:Class , dskern:SignatureOptionWithValue ;
    dskern:argType rdfs:Resource ;
	dskern:commandKeywordValue "graph-uri" ;
	rdfs:label "TOGRAPH" ;
	rdfs:comment "TOGRAPH to indicate target graph"@en ;
    .	
dskern:SignatureOption.MULTITYPE a dskern:SignatureOption ;
	rdfs:label "MULTITYPE" ;
	rdfs:comment "Allows a command to add additional types to an existing URI"@en ;
    .
dskern:SignatureOption.NOCHECK a dskern:SignatureOption ;
	rdfs:label "NOCHECK" ;
	rdfs:comment "Do not check for the resource before adding"@en ;
    .
#some default keywords 
dskern:CommandKeyword.CONTEXT a dskern:CommandKeyword ;
	rdfs:label "CONTEXT" ;
    .
dskern:CommandKeyword.CWD a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
	dskern:argType xsd:string ;
	rdfs:label "CWD" ;
	dskern:commandKeywordValue "working-dir" ;
	rdfs:comment "Directory to set as local for all scripts"@en ;
    .
dskern:CommandKeyword.GRAPH a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
    dskern:argType [ owl:unionOf ( dskern:Graph dskern:pGraph) ] ;
	rdfs:label "GRAPH" ;
	dskern:commandKeywordValue "graph-uri" ;
	rdfs:comment "Uri or name of the graph to set context"@en ;
    .
dskern:CommandKeyword.USER a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
    dskern:argType foaf:Person ;
	rdfs:label "USER" ; 
	dskern:commandKeywordValue "user-uri" ;
	rdfs:comment "Uri of the foaf:Person to set context"@en ;
    .
dskern:CommandKeyword.PASSWORD a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
    dskern:argType xsd:string ;
	rdfs:label "PASSWORD"  ; 
	dskern:commandKeywordValue "user-password" ;
	rdfs:comment "Password of the foaf:Person to set context"@en ;
    .
dskern:CommandKeyword.ROLE a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
    dskern:argType dskern:SecurityContext ;
	rdfs:label "ROLE" ; 
	dskern:commandKeywordValue "role-uri" ;
	rdfs:comment "Uri of the foaf:Group to set context"@en ;
    .
dskern:CommandKeyword.TENANT a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
    dskern:argType dskern:Tenant ;
	rdfs:label "TENANT" ; 
	dskern:commandKeywordValue "tenant-uri" ;
	rdfs:comment "Uri of the tenant to set context"@en ;
    .
dskern:CommandKeyword.VERBOSE a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
	dskern:argType [ owl:oneOf ( "on" "off" "quiet" ) ] ;
	rdfs:label "VERBOSE" ; 
	dskern:commandKeywordValue "on/off/quiet" ;
	rdfs:comment "Control verbosity of console traces"@en ;
    .
dskern:CommandKeyword.RESULTFIRST a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
	dskern:argType [ owl:oneOf ( "on" "off" ) ] ;
	rdfs:label "RESULTFIRST" ; 
	dskern:commandKeywordValue "on/off" ;
	rdfs:comment "Turn on/off resultFirst for query optimization "@en ;
    .
dskern:CommandKeyword.PROFILER a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
	dskern:argType [ owl:oneOf ( "on" "off" ) ] ;
	rdfs:label "PROFILER" ; 
	dskern:commandKeywordValue "on/off" ;
	rdfs:comment "Turn on/off profiling information "@en ;
    .
dskern:CommandKeyword.PROFILER_PATH a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
	dskern:argType xsd:string ;
	rdfs:label "PROFILER_PATH" ;
	dskern:commandKeywordValue "profiling-dir" ;
	rdfs:comment "Directory to set as output for profiling"@en ;
    .
dskern:CommandKeyword.PROFILER_ADD a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
	dskern:argType xsd:string ;
	rdfs:label "PROFILER_ADD" ;
	dskern:commandKeywordValue "categories-to-include" ;
	rdfs:comment "Categories to include in profiling"@en ;
    .
dskern:CommandKeyword.PROFILER_REMOVE a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
	dskern:argType xsd:string ;
	rdfs:label "PROFILER_REMOVE" ;
	dskern:commandKeywordValue "categories-to-exclude" ;
	rdfs:comment "Categories to exclude from profiling"@en ;
    .
dskern:CommandKeyword.DEBUG a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
	dskern:argType [ owl:oneOf ( "on" "off" ) ] ;
	rdfs:label "DEBUG" ; 
	dskern:commandKeywordValue "on/off" ;
	rdfs:comment "Turn on/off debug trace information - for instances call stack trace"@en ;
    .    
dskern:CommandKeyword.VALIDATION a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
	dskern:argType [ owl:oneOf ( "on" "off" ) ] ;
	rdfs:label "VALIDATION" ; 
	dskern:commandKeywordValue "on/off" ;
	rdfs:comment "Turn on/off validation "@en ;
    . 
dskern:CommandKeyword.LANGUAGE a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
	dskern:argType [ owl:oneOf ( "all" "en" "fr" "de" "ja" "zh" ) ] ;
	rdfs:label "LANGUAGE" ;
	dskern:commandKeywordValue "all/en/fr/de/ja/zh" ;
	rdfs:comment "Set language"@en ;
    .
dskern:CommandKeyword.DRIVER a dskern:CommandKeyword ;
	rdfs:label "DRIVER" ;
    . 
dskern:CommandKeyword.ENTAILMENT a rdfs:Class , dskern:OptionalCommandKeywordWithValue ;
	dskern:argType [ owl:oneOf ( "on" "disable" ) ] ;
	rdfs:label "ENTAILMENT" ; 
	dskern:commandKeywordValue "on/disable" ;
	rdfs:comment "Turn on/disable entailment "@en ;
    .
#Signature for set context command
dskern:SetContextSignature a dskern:Signature ;
	dskern:signatureArgs ( 
		dskern:CommandKeyword.CONTEXT
		dskern:CommandKeyword.CWD
		dskern:CommandKeyword.USER
		dskern:CommandKeyword.PASSWORD
		dskern:CommandKeyword.ROLE
		dskern:CommandKeyword.TENANT
		dskern:CommandKeyword.GRAPH
		dskern:CommandKeyword.VERBOSE
		dskern:CommandKeyword.RESULTFIRST
		dskern:CommandKeyword.PROFILER
		dskern:CommandKeyword.PROFILER_PATH
		dskern:CommandKeyword.PROFILER_ADD
		dskern:CommandKeyword.PROFILER_REMOVE
		dskern:CommandKeyword.DEBUG
		dskern:CommandKeyword.VALIDATION
		dskern:CommandKeyword.LANGUAGE
        dskern:CommandKeyword.ENTAILMENT
	) ;
    .
dskern:CommandKeyword.PREFIX a rdfs:Class , dskern:CommandKeywordWithValue ;
	dskern:argType xsd:string ;
	rdfs:label "PREFIX" ; 
	dskern:commandKeywordValue "prefix-string" ;
	rdfs:comment "Prefix string for the namespace"@en ;
    . 
dskern:CommandKeyword.VALUE a rdfs:Class , dskern:CommandKeywordWithValue ;
    dskern:argType xsd:string ;
	rdfs:label "VALUE" ; 
	dskern:commandKeywordValue "namespace-uri" ;
	rdfs:comment "Fully expanded URI"@en ;
    .
dskern:SetPrefixSignature a dskern:Signature ;
	dskern:signatureArgs ( 
		dskern:CommandKeyword.PREFIX
		dskern:CommandKeyword.VALUE
	) ;
	dskern:signatureGraph dskern:WithGraphExpression ;
    .
dskern:Set dskern:signature dskern:SetContextSignature ;
	dskern:signature dskern:SetPrefixSignature ;
    .
dskern:UnsetPrefixSignature a dskern:Signature ;
	dskern:signatureArgs ( 
		dskern:CommandKeyword.PREFIX
	) ;
	dskern:signatureGraph dskern:WithGraphExpression ;
    .
dskern:Unset dskern:signature dskern:UnsetPrefixSignature ;
    .
#Signature for pop context command	
dskern:PopCommandSignature a dskern:Signature ;
	dskern:signatureArgs ( 
		dskern:CommandKeyword.CONTEXT
	) ;
    .
dskern:Pop dskern:signature dskern:PopCommandSignature ;
    .
#Signature for push context command	
dskern:Push dskern:signature dskern:SetContextSignature ;
    .
#Signature for Dataset Add command
dskern:AddCommandSignature a dskern:Signature ;
	dskern:signatureArgs ( 
		[dskern:argType rdfs:Resource; rdfs:label "type-uri"; rdfs:comment "URI of the type to create an instance"@en ] 
	) ;
	dskern:signatureInputValueForm dskern:SignatureInputValueTurtle ;
	dskern:signatureClause dskern:SparqlExpression ;
	dskern:signatureGraph dskern:WithGraphExpression ;
	dskern:signatureOption dskern:SignatureOption.URI ;
	dskern:signatureOption dskern:SignatureOption.PREFIX ;
	dskern:signatureOption dskern:SignatureOption.BLANK ;
	dskern:signatureOption dskern:SignatureOption.FORMAT ;
	dskern:signatureOption dskern:SignatureOption.RETURNTYPE ;
	dskern:signatureOption dskern:SignatureOption.MULTITYPE ;
	dskern:signatureOption dskern:SignatureOption.NOCHECK ;
	dskern:signatureReturnType [
      dskern:argType "instance-uri" ;
      rdfs:comment "URI of the newly added instance"@en
    ] ;
    .
dskern:Add dskern:signature dskern:AddCommandSignature ;
    .

dskern:AddNewCommandSignature a dskern:Signature ;
	dskern:signatureArgs ( 
		[dskern:argType rdfs:Resource; rdfs:label "type-uri"; rdfs:comment "URI of the type to create an instance"@en ] 
	) ;
	dskern:signatureInputValueForm dskern:SignatureInputValueTurtle ;
	dskern:signatureClause dskern:SparqlExpression ;
	dskern:signatureGraph dskern:WithGraphExpression ;
	dskern:signatureOption dskern:SignatureOption.URI ;
	dskern:signatureOption dskern:SignatureOption.PREFIX ;
	dskern:signatureOption dskern:SignatureOption.BLANK ;
	dskern:signatureOption dskern:SignatureOption.FORMAT ;
	dskern:signatureOption dskern:SignatureOption.RETURNTYPE ;
	dskern:signatureReturnType [
      dskern:argType "instance-uri" ;
      rdfs:comment "URI of the newly added instance"@en
    ] ;
    .	
dskern:GraphAdd dskern:signature dskern:AddNewCommandSignature ;
    .
dskern:TenantAdd dskern:signature dskern:AddNewCommandSignature ;
    .
	
#signatures for Dataset Clone command
dskern:CloneCommandSignature a dskern:Signature ;
	dskern:signatureArgs ( 
		[dskern:argType rdfs:Resource; rdfs:label "uri"; rdfs:comment "URI of the type to clone a new instance"@en ] 
	) ;
	dskern:signatureInputValueForm dskern:SignatureInputValueTurtle ;
	dskern:signatureClause dskern:SparqlExpression ;
	dskern:signatureGraph dskern:WithGraphExpression ;
	dskern:signatureOption dskern:SignatureOption.URI ;
	dskern:signatureOption dskern:SignatureOption.PREFIX ;
	dskern:signatureOption dskern:SignatureOption.RETURNTYPE ;
	dskern:signatureOption dskern:SignatureOption.NOCHECK ;
	dskern:signatureReturnType [
      dskern:argType "instance-uri" ; 
      rdfs:comment "URI of the newly cloned instance"@en
    ] ;
    .
dskern:Clone dskern:signature dskern:CloneCommandSignature;
    .
#signatures for Dataset Delete command
